    <sys_ui_action action="INSERT_OR_UPDATE">
        <action_name>x_avnet_04_reyno_0.approve_expense_reque</action_name>
        <active>true</active>
        <client>true</client>
        <client_script_v2><![CDATA[function onClick(g_form) {

    if (g_user.hasRoleExactly('x_avnet_04_reyno_0.Budget reviewer')) {
        var msg = getMessage("This action cannot be undone.");
        g_modal.confirm(getMessage("Approve this request?"), msg, function(confirmed) {
            if (confirmed) {
                var ga = new GlideAjax('x_avnet_04_reyno_0.CostApprovalUtils');
                ga.addParam('sysparm_name', 'approveExpenses');
                ga.addParam('sysparm_recSysId', g_form.getSysId());
                ga.getXML(getAnswer);
            }
        });
    } else {
        var ga = new GlideAjax('x_avnet_04_reyno_0.CostApprovalUtils');
        ga.addParam('sysparm_name', 'approveExpenses');
        ga.addParam('sysparm_recSysId', g_form.getSysId());
        ga.getXML(getAnswer);
    }

    function getAnswer(response) {
        var isApproved = response.responseXML.documentElement.getAttribute("answer");
        g_form.setValue("work_notes", "Request approval submitted.");
        g_form.save();
    }

    return false;
}]]></client_script_v2>
